{
  "redhat.telemetry.enabled": false,
  // Editor
  "editor.fontFamily": "Jetbrains Mono",
  // Make
  "makefile.configureOnOpen": true,
  "git.confirmSync": false,
  // Copilot
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": true,
    "shellscript": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "editor.wordWrap": "on",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.startupEditor": "none",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "remote.autoForwardPortsSource": "hybrid",
  "dotnetAcquisitionExtension.existingDotnetPath": [],
  "git.enableSmartCommit": true,
  "window.systemColorTheme": "dark",
  "git.autofetch": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "chat.agent.maxRequests": 30,
  "terminal.integrated.env.osx": {
    "PATH": "${env:PATH}:/usr/local/share/dotnet"
  },
  "editor.fontLigatures": true,
  "window.openFoldersInNewWindow": "off",
  "projectManager.git.baseFolders": [
    "/Users/jens/proj"
  ],
  "workbench.panel.defaultLocation": "right",
  "editor.mouseWheelZoom": true,
  "editor.tokenColorCustomizations": {
    "[ComfyDark]": {}
  },
  "terminal.external.windowsExec": "",
  "terminal.integrated.mouseWheelZoom": true,
  "csharp.debug.console": "externalTerminal",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.explorerKind": "external",
  "yaml.schemas": {
    "file:///Users/jens/.vscode/extensions/atlassian.atlascode-3.8.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "editor.formatOnSave": true,
  "workbench.preferredLightColorTheme": "Default Dark Modern",
  "docker.extension.enableComposeLanguageServer": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "task.problemMatchers.neverPrompt": true,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffffff",
    "editorCursor.background": "#010101",
  },
  "vscode_vibrancy.opacity": -0.2,
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "ComfyDark",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "accessibility.dimUnfocused.enabled": true,
  "accessibility.dimUnfocused.opacity": 1,
  "editor.stickyScroll.enabled": false,
  "editor.cursorBlinking": "smooth",
  "biome.suggestInstallingGlobally": false,
  "dance.modes": {
    "": {
      "hiddenSelectionsIndicatorsDecoration": {
        "after": {
          "color": "$list.warningForeground"
        },
        "backgroundColor": "$inputValidation.warningBackground",
        "borderColor": "$inputValidation.warningBorder",
        "borderStyle": "solid",
        "borderWidth": "1px",
        "isWholeLine": true
      }
    },
    "input": {
      "cursorStyle": "underline-thin"
    },
    "insert": {
      "decorations": {
        "backgroundColor": "#024a26",
        "isWholeLine": true
      },
      "onLeaveMode": [
        [
          ".selections.save",
          {
            "register": " insert"
          }
        ]
      ]
    },
    "select": {
      "cursorStyle": "block-outline",
      "decorations": {
        "backgroundColor": "#301934",
      }
    },
    "normal": {
      "lineNumbers": "relative",
      "cursorStyle": "block",
      "decorations": {
        "applyTo": "main",
        "backgroundColor": "$editor.hoverHighlightBackground",
        "isWholeLine": true
      },
      "onEnterMode": [
        [
          ".selections.restore",
          {
            "register": " ^",
            "try": true
          }
        ]
      ],
      "onLeaveMode": [
        [
          ".selections.save",
          {
            "register": " ^",
            "style": {
              "borderColor": "$editor.selectionBackground",
              "borderStyle": "solid",
              "borderWidth": "2px",
              "borderRadius": "1px"
            },
            "until": [
              [
                "mode-did-change",
                {
                  "include": "normal"
                }
              ],
              [
                "selections-did-change"
              ]
            ]
          }
        ]
      ]
    }
  },
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 9,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "indentRainbow.indicatorStyle": "light",
  "extensions.ignoreRecommendations": true,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "workbench.editor.showTabs": "none",
  "[csharp]": {
    "editor.defaultFormatter": "nromanov.dotrush"
  },
  "workbench.navigationControl.enabled": false,
  "chat.commandCenter.enabled": false,
  "window.commandCenter": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.layoutControl.enabled": false,
  "editor.matchBrackets": false,
  "cSpell.userWords": [
    "BCDEV",
    "elkds",
    "elkjopnordic",
    "feide",
    "ITDS",
    "multideploy",
    "Pulumi",
    "servicebus",
    "VNET"
  ],
  "chat.tools.terminal.autoApprove": {
    "swift": true
  },
  "workbench.statusBar.visible": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}